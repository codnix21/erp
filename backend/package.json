{
  "name": "erp-backend",
  "version": "1.0.0",
  "description": "ERP System Backend",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "generate:secrets": "node scripts/generate-secrets.js",
    "generate:env": "node generate-env.js",
    "check:user": "node scripts/check-user.js",
    "test:login": "node scripts/test-login.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "erp",
    "backend",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/multipart": "^7.7.3",
    "@fastify/rate-limit": "^9.1.0",
    "@prisma/client": "^5.7.1",
    "@types/nodemailer": "^7.0.4",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "fastify": "^4.24.3",
    "nodemailer": "^7.0.10",
    "pdfkit": "^0.17.2",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "axios": "^1.13.2",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
